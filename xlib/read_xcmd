def read_xcmd
  $cmd = []
  if $gem
    Dir[$gem+"/xcmd/*"].each do |file|
      load file
      l = IO.readlines(file)[1]
      c = l.sub(/\s*def (\S+).*/, '\1').chomp
      $cmd.push(c)
    end
  end
  Dir[Dir.home+"/.xrpn/xcmd/*"].each do |file|
    load file
    l = IO.readlines(file)[1]
    c = l.sub(/\s*def (\S+).*/, '\1').chomp
    $cmd.push(c)
  end
  $cmd.sort!
end
 
