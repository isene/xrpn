def read_xcmd
  $cmd = []
  if $gem
    Dir[$gem+"/xcmd/*"].each do |file|
      load file
      l = File.read(file)
      c = l[/def .*\n/][4..-2]
      $cmd.push(c)
    end
  end
  Dir[Dir.home+"/.xrpn/xcmd/*"].each do |file|
    load file
    l = File.read(file)
    c = l[/def .*\n/][4..-2]
    $cmd.push(c)
  end
  $cmd.sort!
end
 
