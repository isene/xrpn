class XRPN
  def xeq (l)
    page, pos = locate_prg (l)
    if pos == nil
      if l.match(/".*"/)
        return "Error: No such global label: #{l}"
      else
        return "Error: No such label: #{l}"
      end
    else
      if @page != @pg
        @pg  = page
      end
      @rtn.push(@pc) unless $debug
	  p @rtn
      @pc  = pos
      $debug = false
    end
  end
end
