class XRPN
  def prstk
    puts "T = ".color(240) + numformat(@t).color(240,1) + "   Î± = ".color(39)  + @a.to_s.color(39,1)
    puts "Z = ".color(240) + numformat(@z).color(240,1)
    puts "Y = ".color(240) + numformat(@y).color(240,1)
    puts "X = " + "\e[1m"  + numformat(@x)              + "   L = ".color(130) + numformat(@l).color(130,1)
  end
end
 
